Proceso GenerarArregloAleatorio
    Definir cantidad Como Entero
    Definir arreglo Como Entero[]
    Definir i Como Entero
    
    Para i <- 1 Hasta cantidad Hacer
        arreglo[i] <- Aleatorio(1, 100)
    FinPara
    
    Devolver arreglo
FinProceso

Proceso CombinarArreglos
    Definir arreglo1 Como Entero[]
    Definir arreglo2 Como Entero[]
    Definir arregloCombinado Como Entero[]
    Definir i Como Entero
    
    arreglo1 <- GenerarArregloAleatorio(5)
    arreglo2 <- GenerarArregloAleatorio(5)
    
    Para i <- 1 Hasta 5 Hacer
        arregloCombinado.Agregar(arreglo1[i])
        arregloCombinado.Agregar(arreglo2[i])
    FinPara
    
    Escribir "Arreglo combinado:", arregloCombinado
FinProceso

Inicio
    CombinarArreglos()
Fin

